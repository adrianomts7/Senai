programa {
  funcao inicio() {
    // Declarando variaveis
    inteiro numero
  
    // Pedindo dados ao ususario
    escreva("Digite o Número: ")
    leia(numero)

    //Dando a resposta ao usuario
    se(numero == 10){
    escreva("O número digitado é igual a 10")
    }
    
    senao se(numero > 10){
    escreva("O número é maior que 10")
    }
   
    senao
    escreva("O nùmero é Menor que 10")
   
    // Fim do programa
    escreva("\n -=-=-= FIM DO PROGRAMA -=-=-= ")

  }
}
