programa {
  funcao inicio() {
    // Declarando variaveis
    inteiro numero

    // Pedindo dados ao usuario
    escreva("Digite um número: ")
    leia(numero)
    
    // Possivel resultado 
    se(numero >= 10) {
      escreva("O número digitado é maior que 10")
    }
    
    // Outro  possivel Resultado
    senao{
      escreva("O número digitado é menor que 10")
    }
    
    // Fim do programa
    escreva("\n === FIm do Programa ===")
  }
}
